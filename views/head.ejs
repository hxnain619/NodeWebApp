<!DOCTYPE html>
<html>

<head>
  <title><%- h.siteName %></title>
  <!-- <link href='https://api.mapbox.com/mapbox-gl-js/v0.53.0/mapbox-gl.css' rel='stylesheet' /> -->
  <!-- <script src="/javascripts/socket.io.js"></script> -->
  <script src="socket.io/socket.io.js"></script>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="theme-color" content="#000000">

  <!-- for pwa -->
  <link rel="manifest" href="/manifest.json">
  <link rel="apple-touch-icon" sizes="180x180" href="/icons/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/icons/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/icons/favicon-16x16.png">
  <link rel="mask-icon" href="/icons/safari-pinned-tab.svg" color="#c0392b">
  <meta name="msapplication-TileColor" content="#d6d6d6">
  <!-- end pwa -->

  <link rel='stylesheet' href='src/stylesheet/css/index.css' />
  <link rel='stylesheet' href='src/stylesheet/css/index.css.map' />
  <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">

  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css">
  <link rel="stylesheet" href="src/materialize/css/materialize.css">
  <link rel="stylesheet" href="src/materialize/css/materialize.min.css">

</head>

<body>
  <% include ./partials/nav.ejs %>
  <div id="space"></div>
  <div class="container">
    <div class="row ">
      <div class="col s12 m12 l12">



        <% if(flashes) {%>
        <div class="flashes">
          <% console.log(flashes)%>
          <% if(flashes.error && flashes.error.length) {%>
          <% var errorFlashes = flashes.error %>
          <% errorFlashes.forEach(function(error) { %>
          <div class="toast red" role="alert">
            <%= error %>
          </div>
          <% }) %>

          <% } %>
          <% if(flashes.success && flashes.success.length) {%>
          <% var successflashes = flashes.success %>
          <% successflashes.forEach(function(success) { %>
          <div class="toast green" role="alert">
            <%= success %>
          </div>
          <% }) %>

          <% } %>

          <% } %>
        </div>
      </div>
    </div>
  </div>

  <div class="toast" id="offline_div" style="display: none">You are offline</div>